{
  "$id": "resetPasswordAction",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "Reset Password Action",
  "type": "object",
  "properties": {
    "token": {
      "type": "string",
      "minLength": 2,
      "maxLength": 50
    },
    "newPassword": {
      "type": "string",
      "minLength": 8,
      "maxLength": 50
    }
  },
  "required": [
    "token",
    "newPassword"
  ]
}
