{
    "name": "clivern/helium",
    "description": "Fast, Secure and Reliable Newsletter System, Set up in Minutes.",
    "keywords": [
        "clivern",
        "helium"
    ],
    "homepage": "http://clivern.com",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "justinrainbow/json-schema": "^5.2",
        "ramsey/uuid": "^4.2",
        "symfony/amazon-mailer": "6.2.*",
        "symfony/asset": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/event-dispatcher": "6.2.*",
        "symfony/flex": "^1.17|^2",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-foundation": "6.2.*",
        "symfony/mailchimp-mailer": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/mailgun-mailer": "6.2.*",
        "symfony/mailjet-mailer": "6.2.*",
        "symfony/messenger": "6.2.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/oh-my-smtp-mailer": "6.2.*",
        "symfony/password-hasher": "6.2.*",
        "symfony/postmark-mailer": "6.2.*",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/security-csrf": "6.2.*",
        "symfony/sendgrid-mailer": "6.2.*",
        "symfony/sendinblue-mailer": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/yaml": "6.2.*",
        "twig/twig": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "require-dev": {
        "clivern/phpcs": "^1.0",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^6.1"
    }
}
